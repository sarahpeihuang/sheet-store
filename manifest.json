{
    "manifest_version": 3,
    "name": "Google Sheets Quickbar",
    "version": "1.0",
    "permissions": ["storage", "scripting", "activeTab", "contextMenus"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://docs.google.com/spreadsheets/*"],
        "js": ["content.js"]
      }
    ]
  }
  